<h1 class="h">News</h1>

<div class="featured-wrap">

  <% @featured_items.forEach (featured_item) ->: %>

    <div class="featured-media">
      <div class="">
        <h2 class="featured-heading"><%= featured_item.get('name') %></h2>
        <p class=""><%= featured_item.get('description') %></p>
      </div>
    </div>

  <% end %>

</div>
