<div class="card-block">
    <h1>Menus</h1>
</div>

<% @catering_menus.forEach (catering_menu) ->: %>
  <a class="catering-menu-btn" href="#/menu/<%= catering_menu.get('id') %>">
    <div class="card-block catering-menus-block">
      <h2><%= catering_menu.get('name') %> <i class="fa fa-angle-double-right fa-1x"></i></h2>
      <p><%= catering_menu.get('description') %></p>
    </div>
  </a>
<% end %>
