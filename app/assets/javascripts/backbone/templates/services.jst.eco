
  <div class="page-header primary">
    <h1>Our Services</h1>
  </div>

  <div class="page-wrap no-footer">

    <% @service_items.forEach (service_item) ->: %>

    <div class="page-section">
      <h2 class="page-section-title small-title"><%= service_item.get('name') %></h2>
      <div class="page-section-body">
        <p>
          <%= service_item.get('description') %>
        </p>
      </div>
    </div>

    <% end %>

  </div>
